<link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" integrity="sha256-p4NxAoJBhIIN+hmNHrzRCf9tD/miZyoHS5obTRR9BMY=" crossorigin="" />
<script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js" integrity="sha256-20nQCchB9co0qIjJZRGuk2/Z9VM+kNiyxNV1lvTlZBo=" crossorigin=""></script>
{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}


<table class='text-center' >
    <tr>
        <td>{{ form_label(form.type) }}</td>
        <td>{{ form_widget(form.type, {'attr': {'class': 'form-select'}}) }} </td>
    </tr>
    <tr >
        <td >{{ form_errors(form.type,  {'attr': {'class': 'form-error'}}) }}</td>
    </tr>
    <tr>
        <td>{{ form_label(form.lieu) }}</td>
        <td>{{ form_widget(form.lieu, {'attr': {'class': 'form-control'}}) }}  </td>
    </tr>
    <tr>
        <td>{{ form_errors(form.lieu ) }}</td>
    </tr>
    <tr>
        <td>{{ form_label(form.text) }}</td>
        <td>{{ form_widget(form.text) }}</td>
    </tr>
    <tr>  <div class="error-message">
        <td >{{ form_errors(form.text, {'attr': {'class': 'form-error'}}) }}</td></div>
    </tr>
    <tr>
        <td>{{ form_label(form.photo) }}</td>
        <td>{{ form_widget(form.photo, {'attr': {'class': 'form-control'}}) }}</td>
    </tr>
   
</table>

<button type="submit" class="btn btn-primary">Enregistrer</button>
{{form_errors(form)}}
{{ form_end(form) }}
